{
  "name": "hibp",
  "version": "7.1.2-dev.0",
  "description": "A Promise-based API for querying the 'Have I been pwned?' service.",
  "keywords": [
    "haveibeenpwned",
    "hibp",
    "pwned",
    "security",
    "hack",
    "dump",
    "breach",
    "pastes",
    "passwords"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [
    {
      "name": "Daniel Adams",
      "email": "danieladams456@gmail.com"
    }
  ],
  "license": "MIT",
  "main": "lib/hibp.js",
  "module": "es/hibp.js",
  "unpkg": "dist/hibp.min.js",
  "runkitExampleFilename": "example/runkit.js",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist lib es coverage",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "cross-env NODE_ENV=production BABEL_ENV=commonjs babel src --out-dir lib --source-maps --ignore \"src/__mocks__/**/*\",\"**/*.test.js\"",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel src --out-dir es --source-maps --ignore \"src/__mocks__/**/*\",\"**/*.test.js\"",
    "build:umd": "cross-env BABEL_ENV=es rollup --config",
    "build:umd:min": "cross-env NODE_ENV=production BABEL_ENV=es rollup --config",
    "docs": "jsdoc2md src/*.js > API.md",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "lint": "eslint .",
    "prebuild": "npm run clean",
    "precommit": "lint-staged",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "npm run test -- --coverage",
    "test:umd": "npm run build:umd:min && jest --config \"{}\" src/__tests__/umd",
    "test:watch": "npm run test -- --watch"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/hibp.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/hibp/issues"
  },
  "homepage": "https://wkovacs64.github.io/hibp",
  "engines": {
    "node": ">= 6.4.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "jssha": "2.3.1"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.47",
    "@babel/core": "7.0.0-beta.47",
    "@babel/plugin-transform-modules-commonjs": "7.0.0-beta.47",
    "@babel/preset-env": "7.0.0-beta.47",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.0.0-charlie.2",
    "common-tags": "1.7.2",
    "coveralls": "3.0.1",
    "cross-env": "5.1.5",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-jest": "2.1.1",
    "eslint-plugin-import": "2.12.0",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "jsdoc-to-markdown": "4.0.1",
    "lint-staged": "7.1.0",
    "prettier": "1.12.1",
    "puppeteer": "1.4.0",
    "rimraf": "2.6.2",
    "rollup": "0.59.1",
    "rollup-plugin-babel": "4.0.0-beta.4",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "3.0.0"
  }
}
