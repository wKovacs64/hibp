{
  "name": "hibp",
  "version": "5.2.1-dev.0",
  "description": "A Promise-based API for querying the 'Have I been pwned?' service.",
  "keywords": [
    "haveibeenpwned",
    "hibp",
    "pwned",
    "security",
    "hack",
    "dump",
    "breach",
    "pastes",
    "passwords"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "main": "lib/hibp.js",
  "module": "es/hibp.js",
  "unpkg": "dist/hibp.min.js",
  "runkitExampleFilename": "example/runkit.js",
  "scripts": {
    "clean": "rimraf dist lib es coverage",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "cross-env NODE_ENV=production BABEL_ENV=commonjs babel src --out-dir lib --source-maps",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel src --out-dir es --source-maps",
    "build:umd": "cross-env BABEL_ENV=es rollup --config",
    "build:umd:min": "cross-env NODE_ENV=production BABEL_ENV=es rollup --config",
    "docs": "jsdoc2md src/*.js > API.md",
    "format": "prettier-eslint --write \"{src,test,test-umd}/**/*.js\"",
    "lint": "npm run lint:main && npm run lint:test-umd",
    "lint:main": "eslint src test",
    "lint:test-umd": "eslint test-umd",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "test": "cross-env NODE_ENV=test jest --config jest.config.json",
    "test:coverage": "npm run test -- --coverage",
    "test:umd": "npm run build:umd:min && cross-env NODE_ENV=test jest test-umd",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/hibp.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/hibp/issues"
  },
  "homepage": "https://wkovacs64.github.io/hibp",
  "engines": {
    "node": ">= 4"
  },
  "dependencies": {
    "axios": "^0.16.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "jsdoc-to-markdown": "^3.1.0-0",
    "moxios": "^0.4.0",
    "nyc": "^11.2.1",
    "prettier-eslint-cli": "^4.4.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify-es": "^3.1.3"
  },
  "optionalDependencies": {
    "puppeteer": "^0.11.0"
  }
}
