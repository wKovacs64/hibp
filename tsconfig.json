{
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "verbatimModuleSyntax": true,
    "declaration": true,
    "skipLibCheck": false,
    "strict": true,
    "stripInternal": true,
    "newLine": "lf",
    "types": ["node", "vitest/globals"]
  },
  "exclude": ["cypress", "cypress.config.ts"]
}
